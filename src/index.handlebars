        <div id="box-container">
            <div class="wrapper">
                <div id="lid">
                 <div class="front"></div>
                 <div class="back"></div>
                 <div class="top"></div>
                 <div class="top-inside"></div>
                 <div class="bottom"></div>
                 <div class="left"></div>
                 <div class="right"></div>
                </div>
                <div id="box">
                 <div class="front"></div>
                 <div class="back"></div>
                 <div class="top"></div>
                 <div class="bottom"></div>
                 <div class="left"></div>
                 <div class="right"></div>
                 <div class="table-surface"></div>
                </div>
            </div>
        </div>
    </div>
    <div id="content" style="display: none">
        <!-- background of welcome page -->
        <div id="bg-container">
            <div id="bg">
            </div>
        </div>
        <section id="welcome-msg">
            <p lang="ja" data-aos="fade-down">ラミィちゃん一周年おめでとう</p>
            <p lang="ja" data-aos="fade-down">海外ニキから</p>
            <p lang="ja" data-aos="fade-down">ラミィちゃんが私たちからのこの小さなプレゼントを楽しんでください</p>
            <!-- Three arrows (one per span, made with 45deg rotated box with partial border) that animate via CSS -->
            <div id="scroll-arrow-hint">
                <span></span>
                <span></span>
                <span></span>
            </div>
        </section>

        <section id="cover-video">
            <div class="sect-title" data-aos="fade-up">
                <h1>Cover MV</h1>
                <div class="sect-title-deco"></div>
            </div>
            <div id="cover-video-container" data-aos="fade-up">
                <div class="video-container">
                    <iframe title="ラミィちゃんお誕生日おめでとう！" src="https://www.youtube-nocookie.com/embed/0NOcQ9IOX9U"
                        frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                        allowfullscreen></iframe>
                </div>
            </div>
        </section>

